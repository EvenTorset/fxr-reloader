{
  "name": "fxr-reloader",
  "version": "7.1.0",
  "description": "Reload, respawn, and extract FXRs in Elden Ring while the game is running.",
  "type": "module",
  "bin": {
    "reload-fxr": "dist/cli.js"
  },
  "engines": {
    "node": ">=22.4.0"
  },
  "exports": {
    ".": {
      "types": "./dist/reloader.d.ts",
      "default": "./dist/reloader.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch"
  },
  "keywords": [
    "elden-ring",
    "typescript",
    "modding-tools",
    "websocket"
  ],
  "files": [
    "/dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EvenTorset/fxr-reloader.git"
  },
  "bugs": {
    "url": "https://github.com/EvenTorset/fxr-reloader/issues"
  },
  "author": "CCCode",
  "license": "Unlicense",
  "devDependencies": {
    "@types/node": "^22.15.23",
    "typescript": "^5.8.3"
  }
}
