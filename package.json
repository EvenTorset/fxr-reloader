{
  "name": "fxr-reloader",
  "version": "5.0.0",
  "description": "Reload and respawn FXRs in Elden Ring while the game is running.",
  "type": "module",
  "bin": {
    "reload-fxr": "dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/node.d.ts",
      "browser": "./dist/web.js",
      "default": "./dist/node.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch"
  },
  "keywords": [
    "elden-ring",
    "typescript",
    "modding-tools",
    "websocket"
  ],
  "files": [
    "/dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EvenTorset/fxr-reloader.git"
  },
  "bugs": {
    "url": "https://github.com/EvenTorset/fxr-reloader/issues"
  },
  "author": "CCCode",
  "license": "Unlicense",
  "devDependencies": {
    "@types/node": "^22.14.1",
    "@types/ws": "^8.18.1",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "ws": "^8.18.1"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.9"
  }
}
